#include <deque>
#include <iostream>
#include <vector>
#include <memory>
using namespace std;

class Ctest{
    public:
            int a;
            int b;
            int c;
            Ctest(int a, int b, int c ){(*this).a=a;(*this).b=b;(*this).c=c;}
    
    
    
};
Ctest c1(10,20,30);
Ctest c2(10,20,30);
Ctest c3(99,88,77);



int main(){
    vector<Ctest> vCtest{c1,c2,c3};
    shared_ptr<vector<Ctest> >ptr;
    vector<Ctest>::iterator it=vCtest.end()-1;
    
}
